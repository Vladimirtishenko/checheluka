.a-head
	width 100%
	height 125px
	position relative
	z-index 1
	background url('../img/header.png')
	
.a-parth-to-center
	width w
	height 125px
	margin 0 auto
	display flex
	position relative
	justify-content space-between

.a-side-logo
	display flex
	position relative
	align-items center
	padding 0 20px 0 0

.a-side-logo p 
	font-size 12px
	font-family HelveticaRoman
	margin-top -20px
	padding-left 36px
	position relative
	line-height 18px
	color fontg
	&:before
		content ""
		position absolute
		width 0
		height 0
		border-style solid
		top 8px
		line-height 18px
		border-width 8.5px 0 8.5px 13px
		border-color transparent transparent transparent #ffb121
		left 13px

.a-menu-item
	position absolute
	top 5px
	right 20px
	
.a-side-backet__image
	background-color #000
	position relative
	padding 50px 5px 8px
	display block
	&:before
		content ""
		position absolute
		width 39px
		height 50px
		top 0
		left 0
		right 0
		margin auto
		background-image url('../img/sprite.png')
		background-position -363px -4px

.a-side-backet__count, .a-side-backet__price
	color #fff 
	font-size 12px 
	display block
	padding 2px 10px
	white-space nowrap
	text-align center

.a-time-to-start
	padding 15px 15px 0 15px
	position absolute
	text-align center
	background green
	color #fff
	bottom -45px
	left 50%

.a-time-to-start span 
	font-family HelveticaBold
	font-size 52px
	line-height 60px
	
.a-time-to-start p 
	font-family HelveticaRoman
	font-size 14px